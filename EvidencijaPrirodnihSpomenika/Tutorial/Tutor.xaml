            <Image x:Name="Slika"  Width="355" Height="146" Canvas.Top="181" Canvas.Left="3"/>
        </Canvas>
        <Button  x:Name="Close" Content="Izadji" Margin="284,342,10.667,10.333" Click="Close_Click" />
        <Button x:Name="Ikonica" Content="Prikazi dodavanje" Margin="10,342,245.667,10.333" Click="Ikonica_Click" Visibility="Hidden"  />
        <Button  x:Name="CloseSlika" Content="Povratak" Margin="284,342,10.667,10.333" Click="CloseSlika_Click_1" Visibility="Hidden"  />
    </Grid>
</Window>
