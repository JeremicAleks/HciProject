        xmlns:local="clr-namespace:EvidencijaPrirodnihSpomenika.Tutorial"
        mc:Ignorable="d"
        Title="Tutorijal Dodavanje spomenika" Height="412.334" Width="368" WindowStyle="ToolWindow" WindowStartupLocation="Manual" Left="900" Top="100" ResizeMode="NoResize">
